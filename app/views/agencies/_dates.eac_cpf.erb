<% if fromDate && toDate %>
  <dateRange>
    <fromDate standardDate="<%= fromDate %>"><%= dateStart %></fromDate>
    <toDate standardDate="<%= toDate %>"><%= dateEnd %></toDate>
  </dateRange>
<% elsif fromDate %>
  <date standardDate="<%= fromDate %>" notBefore="<%= fromDate %>"><%= dateStart %></date>
<% elsif toDate %>
  <date standardDate="<%= toDate %>" notAfter="<%= toDate %>"><%= dateEnd %></date>
<% end %>
