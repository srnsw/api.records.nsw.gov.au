<% if fromDate && toDate %>
    <dateRange>
       <fromDate standardDate="<%= fromDate %>"><%= fromDate %><%= dateStart %></fromDate>
        <toDate standardDate="<%= toDate %>"><%= dateEnd %></toDate>
        </dateRange>
      </existDates>
    <% elsif fromDate %>
        <date standardDate="<%= fromDate %>" notBefore="<%= fromDate %>"><%= fromDate %><%= dateStart %></date>
    <% elsif toDate %>
        <date standardDate="<%= toDate %>" notAfter="<%= fromDate %>"><%= dateEnd %></date>
    <% end %>
