<h1>Access Directions</h1>

<% if @accessdirs %>
	<% @accessdirs.each do |accessdir| %>
		<%= link_p "#{accessdir.id} - #{accessdir.agency_title}", accessdir %>
	<% end %>
	<%= paginate @accessdirs, :browse_uri => nil %>
<% end %>

<% content_for :title do %>
	<% "Access Directions" %>
<% end %>