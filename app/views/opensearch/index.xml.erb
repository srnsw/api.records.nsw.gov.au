<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:srnsw-custom="http//www.records.nsw.gov.au/StateRecordsNSWOpenSearchExtensions" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>State Records NSW search</ShortName>
  <Description>Search the collection of the State Records Authority of New South Wales</Description>
  <% search_string = root_url + 'search?q={searchTerms}&page={startPage?}&count={count?}' %>
  <InputEncoding>UTF-8</InputEncoding>
  <Url type="application/atom+xml" template="<%= search_string + '&format=xml' %>"/>
  <Url type="text/html" template="<%= search_string %>"/>
  <Image height="16" width="16" type="image/vnd.microsoft.icon"><%= root_url + "favicon.ico" %></Image>
  <Query role="example" searchTerms="coal" />
  <SyndicationRight>open</SyndicationRight>
  <AdultContent>false</AdultContent>
  <Language>en-au</Language>
  <moz:SearchForm><% root_url %></moz:SearchForm>
</OpenSearchDescription>
