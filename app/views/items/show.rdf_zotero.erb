<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
 xmlns:z="http://www.zotero.org/namespaces/export#"
 xmlns:bib="http://purl.org/net/biblio#"
 xmlns:foaf="http://xmlns.com/foaf/0.1/"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:dcterms="http://purl.org/dc/terms/">
    <bib:Manuscript rdf:about="<%= url_for(:only_path => false) %>">
        <z:itemType>manuscript</z:itemType>
        <bib:authors>
            <rdf:Seq>
                <rdf:li>
                    <foaf:Person>
                        <foaf:surname></foaf:surname>
                        <foaf:givenname></foaf:givenname>
                    </foaf:Person>
                </rdf:li>
            </rdf:Seq>
        </bib:authors>
        <dc:title><%= @item.Item_title %></dc:title>
        <dc:date><%= @item.Start_date %></dc:date>
        <dc:identifier>
            <dcterms:URI>
                <rdf:value><%= url_for(:only_path => false) %></rdf:value>
            </dcterms:URI>
        </dc:identifier>
        <z:archive>State Records NSW</z:archive>
        <dc:coverage>NUA <%= @item.series.Series_number %>, <%= @item.Item_number_or_control_symbol %></dc:coverage>
        <z:libraryCatalog>http://www.records.nsw.gov.au</z:libraryCatalog>
    </bib:Manuscript>
</rdf:RDF>
