<% content_for :head do %><link rel="unapi-server" type="application/x-rdf_zotero+xml" title="unAPI"
    href="<%= url_for(:action => 'index', :controller=> 'unapi', :only_path => false)%>" />
<% end %>
<abbr class="unapi-id" title="<%= url_for(:only_path => false) %>"></abbr>

<h1>Items</h1>
<% @items.each do |item| %>
<%= link_p item.Item_title, item %>
<% end %>
<%= paginate @items, :browse_uri => nil %>

<% content_for :title do %>
<% "Items" %>
<% end %>
