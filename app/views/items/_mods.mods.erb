<titleInfo>
  <title><%= mods.Item_title %></title>
</titleInfo>
<typeOfResource manuscript="yes" collection="yes">mixed material</typeOfResource>
<genre authority="local">manuscript</genre>
<% mods.series.creating_agencies.each do |agency| %>
<name type="personal">
  <namePart><%= agency.Agency_title %></namePart>
  <role>
    <roleTerm type="code" authority="marcrelator">aut</roleTerm>
   </role>
</name>
<% end %>
<% mods.series.persons.each do |person| %>
<name type="personal">
  <namePart type="family"><%= person.Surname %></namePart>
  <namePart type="given"><%= person.Given_names %></namePart>
  <role>
    <roleTerm type="code" authority="marcrelator">aut</roleTerm>
   </role>
</name>
<% end %>
<originInfo>
  <dateCreated><%= date_simple(mods.Start_date) %></dateCreated>
</originInfo>
<location>
  <physicalLocation>State Records NSW</physicalLocation>
  <shelfLocator>NUA <%= mods.series.Series_number %><% if has_content? mods.Item_number_or_control_symbol %>, <%= mods.Item_number_or_control_symbol %><% end %></shelfLocator>
  <url><%= url_for :action => "show", :controller => "items", :id => mods, :only_path => false %></url>
</location>
