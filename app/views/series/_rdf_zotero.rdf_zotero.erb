<bib:Manuscript rdf:about="<%=  url_for :action => "show", :controller => "series", :id => rdf_zotero, :only_path => false %>">
  <z:itemType>manuscript</z:itemType>
  <bib:authors>
    <rdf:Seq>
      <% rdf_zotero.creating_agencies.each do |agency| %>
        <rdf:li>
          <foaf:Person>
            <foaf:surname><%= agency.Agency_title %></foaf:surname>
            <foaf:givenname></foaf:givenname>
          </foaf:Person>
        </rdf:li>
      <% end %>
      <% rdf_zotero.persons.each do |person| %>
        <rdf:li>
          <foaf:Person>
            <foaf:surname><%= person.Surname %></foaf:surname>
            <foaf:givenname><%= person.Given_names %></foaf:givenname>
          </foaf:Person>
        </rdf:li>
      <% end %>
    </rdf:Seq>
  </bib:authors>
  <dc:title><%= rdf_zotero.Series_title %></dc:title>
  <dc:date><%= date_simple(rdf_zotero.Start_date) %></dc:date>
  <dc:identifier>
    <dcterms:URI>
      <rdf:value><%= url_for :action => "show", :controller => "series", :id => rdf_zotero, :only_path => false %></rdf:value>
    </dcterms:URI>
  </dc:identifier>
  <z:archive>State Records NSW</z:archive>
  <dc:coverage>NRS <%= rdf_zotero.Series_number %></dc:coverage>
  <z:libraryCatalog>http://www.records.nsw.gov.au</z:libraryCatalog>
</bib:Manuscript>
